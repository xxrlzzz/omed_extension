{
    "manifest_version": 3,
    "name": "OME2",
    "description": "从chromium跳转到ide",
    "version": "1.0",
    "permissions": [
        "contextMenus",
        "tabs",
        "storage",
        "scripting"
    ],
    "action": {
        "default_icon": "iconv2.png",
        "default_title": "omed",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "host_permissions": [
        "http://127.0.0.1:8989/*",
        "https://source.chromium.org/*",
        "https://cs.android.com/*"
    ]
}